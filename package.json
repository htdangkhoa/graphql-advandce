{
  "name": "easy-graphql",
  "version": "1.0.0",
  "main": "server.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "server": "nodemon server/server.js --exec babel-node",
    "client": "preact watch ./client",
    "build:server": "babel server -d dist && npm run minify",
    "build:client": "preact build ./client --no-prerender",
    "build": "npm run build:server && npm run build:client",
    "minify": "minify dist -d dist",
    "start": "node dist/server.js",
    "dummy:data": "babel-node dummyData.js --presets es2017,stage-3,node8"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta.2",
    "compression": "^1.7.1",
    "connect-timeout": "^1.9.0",
    "consolidate": "^0.15.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.14.1",
    "helmet": "^3.9.0",
    "moment": "^2.20.1",
    "mongoose": "^5.0.0-rc0",
    "morgan": "^1.9.0",
    "mustache": "^2.3.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.6.0",
    "react-icons": "^2.2.7",
    "reactstrap": "next",
    "styled-components": "^2.4.0",
    "uuid": "^3.1.0"
  }
}
